<%
  if (typeof title === 'undefined' || title === ''){
    var pageTitle = siteTitle;
  }else{
    var pageTitle = title + " | " + siteTitle;
  }
  if (typeof pageOgp === 'undefined') { var ogp = ogp; }
  if (typeof pageKeyword === 'undefined') { var pageKeyword = keyword; }
  if (typeof pageDescription === 'undefined') { var pageDescription = description; }
%>
